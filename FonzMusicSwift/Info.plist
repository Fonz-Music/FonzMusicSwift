	<string>to get the coaster uid to send to our api</string>
	<key>UIAppFonts</key>
	<array>
		<string>MuseoSans-100.otf</string>
		<string>MuseoSans-300.otf</string>
		<string>MuseoSans-500.otf</string>
		<string>MuseoSans-700.otf</string>
	</array>
